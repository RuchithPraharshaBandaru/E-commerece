<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>EcoWear Collective</title>
  <script src="https://cdn.tailwindcss.com"></script>
</head>
<body class="bg-gradient-to-br from-emerald-50 via-green-100 to-emerald-200 min-h-screen font-sans">

  <header class="bg-white shadow-sm sticky top-0 z-20">
    <div class="max-w-7xl mx-auto flex items-center justify-between px-6 py-4">
      <!-- Logo -->
      <div class="flex items-center gap-2">
        <span class="inline-block w-8 h-8 bg-emerald-500 rounded-full"></span>
        <span class="text-2xl font-bold text-emerald-700 tracking-wide">Swift Mart</span>
      </div>
      <!-- Navigation -->
      <nav class="hidden md:flex gap-8">
        <a href="#" class="text-gray-700 hover:text-emerald-600">Home</a>
        <a href="#" class="text-gray-700 hover:text-emerald-600">About</a>
        <a href="#" class="text-gray-700 hover:text-emerald-600">Dashboard</a>
      </nav>
      <!-- Button group for icons -->
      <div class="flex items-center space-x-2">
        <!-- Cart Icon Button -->
        <button class="bg-emerald-600 hover:bg-emerald-700 text-white p-2 rounded-lg transition" aria-label="Cart">
          <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 3h2l.4 2M7 13h10l4-8H5.4M7 13L5.4 5M7 13l-2 9m13-9v9m-6 0a2 2 0 104 0 2 2 0 00-4 0z" />
          </svg>
        </button>
        <!-- Profile Icon Button -->
        <button class="bg-emerald-700 hover:bg-emerald-800 text-white p-2 rounded-lg transition" aria-label="Profile">
          <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5.121 17.804A8.966 8.966 0 0112 15c1.657 0 3.21.402 4.879 1.117M15 10a3 3 0 11-6 0 3 3 0 016 0z" />
          </svg>
        </button>
      </div>
    </div>
  </header>
  
  <!-- Hero Section -->
  <section class="relative flex flex-col md:flex-row items-center max-w-7xl mx-auto px-6 py-12 mb-10">
    <div class="md:w-1/2 mb-10 md:mb-0">
      <h1 class="text-5xl font-extrabold text-gray-800 mb-4 leading-tight">Sustainable Looks, <span class="text-emerald-600">Conscious Choices</span></h1>
      <p class="text-lg text-gray-600 mb-6">Curated eco-friendly fabrics and responsibly crafted fashion. Shop stylishly, live sustainably.</p>
      <a href="#order" class="inline-block bg-emerald-600 text-white px-6 py-3 rounded-full font-semibold hover:bg-emerald-700 transition">Browse Collection</a>
    </div>
    <div class="md:w-1/2 flex justify-center">
      <img src="https://images.unsplash.com/photo-1506744038136-46273834b3fb?auto=format&fit=crop&w=400&q=80" alt="Eco Fashion" class="rounded-2xl shadow-xl w-80 h-80 object-cover border-4 border-emerald-100">
    </div>
    <!-- Decorative Accent -->
    <div class="absolute top-0 left-0 w-32 h-32 bg-emerald-100 rounded-full -z-10 blur-2xl"></div>
    <div class="absolute bottom-0 right-0 w-48 h-48 bg-emerald-200 rounded-full -z-10 blur-2xl"></div>
  </section>

  <div class="max-w-7xl mx-auto flex flex-col md:flex-row gap-10 px-6">

    <!-- Sidebar Fabric Selector -->
    <aside class="md:w-1/4 mb-10 md:mb-0">
      <h2 class="text-xl font-bold text-emerald-800 mb-6">Filter by Fabric</h2>
      <ul class="space-y-4">
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://img.freepik.com/free-photo/top-view-cloths-made-with-different-natural-pigments-composition_23-2148734249.jpg" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100">
            <span class="font-medium text-gray-700">Cotton</span>
          </button>
        </li>
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://plus.unsplash.com/premium_photo-1672681446484-827767f95dab?q=80&w=686&auto=format&fit=crop" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100">
            <span class="font-medium text-gray-700">Silk</span>
          </button>
        </li>
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://media.istockphoto.com/id/453550893/photo/sample-a-good-quality-leather-in-various-colors.jpg?s=612x612&w=is&k=20&c=gQUo-0fsKkEScdIMfPq48qFJT5dzih6XrttNGYP0SkE=" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100">
            <span class="font-medium text-gray-700">Leather</span>
          </button>
        </li>
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://images.unsplash.com/photo-1636716018019-569382b46111?q=80&w=1170&auto=format&fit=crop" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100">
            <span class="font-medium text-gray-700">Wool</span>
          </button>
        </li>
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://plus.unsplash.com/premium_photo-1674575954775-b5ef5af6fd17?w=1000&auto=format&fit=crop&q=60" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100">
            <span class="font-medium text-gray-700">Linen</span>
          </button>
        </li>
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://img.freepik.com/free-photo/close-up-flannel-shirt-detail_23-2149575336.jpg?ga=GA1.1.1660626605.1746341889&semt=ais_hybrid&w=740" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100" alt="Cashmere">
            <span class="font-medium text-gray-700">Cashmere</span>
          </button>
        </li>
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://plus.unsplash.com/premium_photo-1675877946243-bc3f83e65afe?w=1000&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MXx8ZGVuaW0lMjBmYWJyaWN8ZW58MHx8MHx8fDA%3D" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100" alt="Denim">
            <span class="font-medium text-gray-700">Denim</span>
          </button>
        </li>
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://plus.unsplash.com/premium_photo-1672680445357-4dc4d3b2756e?w=1000&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MXx8cG9seWVzdGVyfGVufDB8fDB8fHww" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100" alt="Polyester">
            <span class="font-medium text-gray-700">Polyester</span>
          </button>
        </li>
        <li>
          <button class="flex items-center w-full gap-3 p-3 rounded-lg bg-white shadow hover:bg-emerald-50 transition">
            <img src="https://media.istockphoto.com/id/1186706715/photo/close-up-fabric-fiber-in-underwater.jpg?s=612x612&w=0&k=20&c=OQ-gl9rSaTyAFd49-H5cib_tJEHzXENBLGUSoM8R7aw=" class="w-10 h-10 rounded-full object-cover border-2 border-emerald-100" alt="Synthetic">
            <span class="font-medium text-gray-700">Synthetic</span>
          </button>
        </li>
        
      </ul>
    </aside>

    <!-- Order Table Section -->
    <main class="md:w-3/4" id="order">
      <h2 class="text-2xl font-bold text-emerald-800 mb-6">Order Your Sustainable Wear</h2>
      <div class="overflow-x-auto">
        <table class="min-w-full bg-white rounded-xl shadow text-gray-700">
          <thead>
            <tr class="bg-emerald-100 text-emerald-800 text-center">
              <th class="py-3 px-4">Material</th>
              <th class="py-3 px-4">Cloth Duration</th>
              <th class="py-3 px-4">Size</th>
              <th class="py-3 px-4">Available Stock</th>
              <th class="py-3 px-4">Quantity</th>
              <th class="py-3 px-4">Price per Item</th>
              <th class="py-3 px-4">Add to Cart</th>
            </tr>
          </thead>
          <tbody>
            <% combinations.forEach(function(comb) { %>
              <tr class="border-b hover:bg-green-50 text-center">
                <td class="py-3 px-4 flex items-center justify-center gap-2">
                  <span class="ml-2 font-medium"><%= comb.fabric %></span>
                </td>
                <td class="py-3 px-4"><%= comb.usageDuration %></td>
                <td class="py-3 px-4"><%= comb.size %></td>
                <td class="py-3 px-4"><%= comb.quantity %></td>
                <td class="py-3 px-4"><%= comb.estimated_value%></td>
                <td class="py-3 px-4">
                  <div class="flex items-center justify-center gap-2">
                    <button class="w-8 h-8 rounded-full bg-emerald-100 text-emerald-700 font-bold hover:bg-emerald-200">-</button>
                    <span>1</span>
                    <button class="w-8 h-8 rounded-full bg-emerald-100 text-emerald-700 font-bold hover:bg-emerald-200">+</button>
                  </div>
                </td>
                <td class="py-3 px-4">
                  <button class="bg-emerald-600 text-white px-4 py-2 rounded-lg hover:bg-emerald-700">Add to Cart</button>
                </td>
              </tr>
            <% }) %>
          </tbody>          
        </table>
      </div>
    </main>
  </div>

  <!-- Footer -->
  <footer class="bg-white border-t py-6 mt-16">
    <div class="max-w-7xl mx-auto text-center text-gray-500 text-sm">
      &copy; 2025 Swift Mart. Crafted for a greener tomorrow.
    </div>
  </footer>
</body>
</html>


